# Gráfico e Analise de Correlação
library(tidyverse)
library(ggplot2)
mtcars
?mtcars
# Eixo X - Milhas por galão (MPG)
# Eixo Y - Peso do Carro (WT)
mtcars %>%
ggplot(aes(x = wt, y = mpg)) +
geom_point()
# Calculo de correlação
# r = 1 : Correlação diretamente proporcional
# r = 0 : Ausência de correlação
# r = -1 : Correlação inversamente proprocional
cor(mtcars$wt, mtcars$mpg)
# Correlação de todas as váriaveis
cor(mtcars)
# Gráfico de Matriz de correlação
library(ggcorrplot)
correl = cor(mtcars)
ggpcorrplot(correl)
